{
  "name": "rust-wasm-react-base",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "19.7.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^5.8.4",
    "@mantine/hooks": "^5.8.4",
    "@mantine/modals": "^5.8.4",
    "@mantine/spotlight": "^5.8.4",
    "@tabler/icons": "^1.114.0",
    "eslint-plugin-cypress": "^2.12.1",
    "react": "^18.2.0",
    "react-bootstrap-icons": "^1.10.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "serve": "^14.1.2",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^2.2.0",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^3.2.3",
    "vite-plugin-wasm-pack": "^0.1.9"
  },
  "scripts": {
    "prepare": "husky install",
    "wasm": "wasm-pack build ./wasm --target web",
    "dev": "yarn run wasm && vite --host",
    "build": "yarn run wasm && tsc && vite build",
    "preview": "view preview",
    "serve": "serve -s dist -p 3000",
    "test": "yarn run test:cypress && yarn run test:wasm",
    "test:wasm": "cd wasm && cargo test",
    "test:cypress": "yarn cypress run",
    "lint:js:check": "eslint . --config ./.eslintrc.json",
    "lint:js": "eslint --fix . --config ./.eslintrc.json",
    "lint:wasm:check": "cd wasm && cargo clippy -- -D warnings",
    "lint:wasm": "cd wasm && cargo clippy --fix",
    "format:check": "prettier --check . --config ./.prettierrc.json",
    "format": "prettier --write . --config ./.prettierrc.json"
  }
}
